import rtems_waf.rtems as rtems

def build(bld):
    rtems.build(bld)

    bld(features = 'c cprogram',
        target = 'psx_pthread_report.exe',
        source = ['pthread_attr_report.c','rtems_config.c'],
        lib = ['c'])

